{
  "name": "@ionic/cli-utils",
  "version": "0.0.8",
  "description": "Ionic CLI Utils",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run clean && ../../node_modules/.bin/webpack",
    "clean": "rm -rf ./dist",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "watch": "npm run clean && ../../node_modules/.bin/tsc --watch"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "license": "MIT",
  "dependencies": {
    "archiver": "^1.3.0",
    "cross-spawn": "^4.0.2",
    "inquirer": "^1.2.2",
    "superagent": "^2.3.0",
    "leek": "0.0.24"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "os-name": "^2.0.1",
    "progress": "^1.1.8",
    "rimraf": "^2.5.4",
    "uuid": "^3.0.1"
  },
  "files": [
    "dist/"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$",
    "testResultsProcessor": "<rootDir>/../../node_modules/ts-jest/coverageprocessor.js"
  }
}
