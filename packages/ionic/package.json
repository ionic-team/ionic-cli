{
  "name": "ionic",
  "version": "3.0.0-beta.4",
  "preferGlobal": true,
  "description": "A tool for creating and developing Ionic Framework mobile apps.",
  "homepage": "http://ionicframework.com/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io) ",
  "bin": {
    "ionic": "./bin/ionic"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "./dist/ionic.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "../../node_modules/.bin/tslint -c ../../tslint.json 'src/**/*.ts'",
    "build": "npm run clean && ../../node_modules/.bin/webpack",
    "clean": "rm -rf ./dist",
    "changelog": "../../node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "../../node_modules/.bin/jest",
    "globallink": "npm link"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "keywords": [
    "ionic",
    "ionic framework",
    "ionicframework",
    "mobile",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/driftyco/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/driftyco/ionic-cli/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.3",
    "minimist": "^1.2.0",
    "opn": "^4.0.2",
    "path-exists": "^3.0.0",
    "tar": "^2.2.1"
  },
  "dependencies": {
    "@ionic/cli-utils": "^0.0.8",
    "node-fetch": "^1.6.3",
    "qrcode-terminal": "^0.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$",
    "testResultsProcessor": "<rootDir>/../../node_modules/ts-jest/coverageprocessor.js"
  }
}
