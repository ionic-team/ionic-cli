{
  "private": true,
  "name": "cli-scripts",
  "version": "1.0.9",
  "bin": {
    "ionic-cli-scripts": "./bin/ionic-cli-scripts"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --project tsconfig.json",
    "build": "npm run clean && tsc",
    "watch": "tsc -w --preserveWatchOutput",
    "test": "jest --maxWorkers=4",
    "prepublishOnly": "npm run build"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": "npm run lint"
    },
    "ignore": [
      "**/__tests__/*.{ts,tsx}"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@ionic/cli-framework": "2.1.2",
    "@ionic/utils-fs": "2.0.3",
    "ansi-styles": "^4.0.0",
    "chalk": "^2.3.0",
    "escape-string-regexp": "^2.0.0",
    "ionic": "5.2.3",
    "strip-ansi": "^5.2.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/ansi-styles": "^3.2.0",
    "@types/jest": "^24.0.3",
    "@types/node": "~8.9.4",
    "jest": "^23.0.1",
    "jest-cli": "^24.1.0",
    "lint-staged": "^9.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.1",
    "tslint": "^5.9.1",
    "typescript": "~3.5.1"
  }
}
