{
  "private": true,
  "name": "cli-scripts",
  "version": "0.0.12",
  "bin": {
    "ionic-cli-scripts": "./bin/ionic-cli-scripts"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --config tslint.js --project tsconfig.json",
    "build": "npm run clean && tsc",
    "watch": "tsc -w --preserveWatchOutput",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": "npm run lint"
    },
    "ignore": [
      "**/__tests__/*.{ts,tsx}"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@ionic/cli-framework": "1.0.2",
    "@ionic/cli-utils": "2.0.3",
    "ansi-styles": "^3.2.0",
    "chalk": "^2.3.0",
    "escape-string-regexp": "^1.0.5",
    "ionic": "4.0.3",
    "strip-ansi": "^4.0.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/ansi-styles": "^3.2.0",
    "@types/escape-string-regexp": "1.0.0",
    "@types/node": "^6.0.101",
    "@types/strip-ansi": "^3.0.0",
    "jest": "^23.0.1",
    "jest-cli": "^23.0.1",
    "lint-staged": "^7.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "tslint": "^5.9.1",
    "typescript": "~3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
