{
  "name": "@ionic/schematics-angular",
  "version": "1.0.6",
  "description": "Schematics specific to Ionic for Angular",
  "homepage": "https://ionicframework.com/",
  "author": "Ionic Team <hi@ionic.io> (https://ionicframework.com) ",
  "scripts": {
    "clean": "rimraf '*/index.{js,d.ts}'",
    "lint": "tslint --config tslint.js --project tsconfig.json",
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": "npm run lint"
    },
    "ignore": [
      "**/__tests__/*.{ts,tsx}"
    ]
  },
  "keywords": [
    "blueprints",
    "code generation",
    "schematics",
    "ionic",
    "ionic framework",
    "ionicframework"
  ],
  "schematics": "./collection.json",
  "dependencies": {
    "@schematics/angular": "0.9.0-beta.3",
    "lodash": "^4.17.5",
    "tslib": "^1.9.0",
    "typescript": "3.0.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.9.0-beta.3",
    "@angular-devkit/schematics": "0.9.0-beta.3",
    "@types/lodash": "^4.14.104",
    "@types/node": "^6.0.101",
    "jest": "^23.0.1",
    "jest-cli": "^23.0.1",
    "lint-staged": "^7.2.0",
    "rimraf": "^2.6.2",
    "source-map": "^0.7.0",
    "ts-jest": "~23.1.0",
    "tslint": "^5.9.1"
  },
  "peerDependencies": {
    "@angular-devkit/core": "0.9.0-beta.3",
    "@angular-devkit/schematics": "0.9.0-beta.3"
  }
}
