{
  "name": "@ionic/cli-utils",
  "version": "1.8.1",
  "description": "Ionic CLI Utils",
  "homepage": "https://ionic.io/",
  "author": "Ionic Team <hi@ionic.io> (http://ionic.io)",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rm -rf index.* bootstrap.* definitions.* guards.* ./lib ./commands ./dist",
    "lint": "tslint --config ../../../tslint.js --project tsconfig.json --type-check",
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ionic",
    "cordova",
    "ionic framework",
    "ionicframework",
    "ionic cordova",
    "mobile",
    "backend",
    "app",
    "hybrid",
    "cordova",
    "native",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/ionic-cli/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@types/gulp": "^3.8.33",
    "archiver": "^2.0.0",
    "chalk": "^2.0.0",
    "chokidar": "^1.7.0",
    "ci-info": "^1.0.0",
    "cross-spawn": "^5.1.0",
    "dargs": "^5.1.0",
    "diff": "^3.3.0",
    "elementtree": "^0.1.7",
    "express": "^4.15.4",
    "inquirer": "^3.2.1",
    "leek": "0.0.24",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "opn": "^5.1.0",
    "proxy-middleware": "^0.15.0",
    "semver": "^5.4.1",
    "slice-ansi": "^1.0.0",
    "ssh-config": "^1.0.1",
    "string-width": "^2.1.1",
    "strip-ansi": "^4.0.0",
    "superagent": "^3.5.2",
    "tar": "^2.2.1",
    "tiny-lr": "^1.0.5",
    "tslib": "^1.7.1",
    "uuid": "^3.0.1",
    "wrap-ansi": "^3.0.1"
  },
  "devDependencies": {
    "@angular/compiler": "^4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@ionic/cli-scripts": "0.3.3",
    "@types/archiver": "^2.0.0",
    "@types/chalk": "^0.4.31",
    "@types/chokidar": "^1.7.1",
    "@types/clean-css": "^3.4.30",
    "@types/diff": "^3.2.0",
    "@types/express": "^4.0.36",
    "@types/inquirer": "0.0.35",
    "@types/lodash": "^4.14.71",
    "@types/minimist": "^1.2.0",
    "@types/ncp": "^2.0.0",
    "@types/opn": "^3.0.28",
    "@types/semver": "^5.3.33",
    "@types/tar": "^1.0.29",
    "@types/uuid": "^3.4.0",
    "rxjs": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts)": "<rootDir>/../../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|js)$"
  }
}
